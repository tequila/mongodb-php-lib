{
    "name": "tequila/mongodb-php-lib",
    "description": "PHP MongoDB high-level driver library",
    "type": "library",
    "require": {
        "php": ">=5.6",
        "ext-mongodb": ">=1.1.0",
        "tequila/options-resolver": "dev-master",
        "tequila/mongodb-driver-wrapper": "dev-master"
    },
    "autoload": {
        "psr-4": { "Tequila\\MongoDB\\": "src/" },
        "files": [ "src/functions.php" ]
    },
    "autoload-dev": {
        "psr-4": { "Tequila\\MongoDB\\Tests\\": "tests/" }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Petr Buchyn",
            "email": "petrbuchyn@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^5.4"
    }
}
